{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "eslint-disable": 0,
      "eslint-disable-line": 0,
      "eslint-disable-next-line": 0
    }
}
